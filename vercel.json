{
    "version": 2,
    "projects": {
      "api": {
        "src": "apps/api/src/index.ts",
        "use": "@vercel/node"
      },
      "web": {
        "src": "apps/web",
        "use": "@vercel/next"
      }
    },
    "builds": [
      {
        "src": "apps/api/src/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "apps/web/next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "apps/api/src/index.ts" },
      { "src": "/(.*)", "dest": "apps/web/$1" }
    ]
  }
  